<UserControl x:Class="Genesyslab.Desktop.Modules.YoutubeWorkItem.Windows.ContentView.YoutubeContactDetailContentView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:Controls="clr-namespace:Genesyslab.Desktop.Modules.YoutubeWorkItem.Controls">

    <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto"
                   HorizontalAlignment="Stretch" VerticalAlignment="Stretch">

        <Controls:YoutubeView x:Name="MainYoutubeView" DataContext="{Binding YoutubeData}" Loaded="MainYoutubeView_Loaded"/>

    </ScrollViewer>
</UserControl>
